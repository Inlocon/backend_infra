{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowAllForSelectedServices",
      "Effect": "Allow",
      "Action": [
        "acm:*",
        "application-autoscaling:*",
        "cloudtrail:*",
        "logs:*",
        "ec2:*",
        "ecr:*",
        "ecs:*",
        "elasticloadbalancing:*",
        "iam:*",
        "rds:*",
        "s3:*",
        "secretsmanager:*",
        "ssm:*"
      ],
      "Resource": "*"
    }]
}

